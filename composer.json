{
    "name": "loca/jsonclient",
    "description": "Simple Json client plugin for Craft3 CMS. The plugin provides a simple Twig extension which allows you to fetch a Json url and return it to your Twig template.",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "json",
        "jsonclient",
        "twig",
        "craft3"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hong Xuan",
            "email": "xuanvan229@gmail.com",
            "homepage": "https://github.com/xuanvan229"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.1"
    },
    "autoload": {
        "psr-4": {
          "loca\\jsonclient\\": "src/"
        }
    },
    "extra": {
        "name": "Json Client",
        "handle": "jsonclient",
        "schemaVersion": "1.0.0",
        "developer": "Hong Xuan",
        "developerUrl": "https://github.com/xuanvan229",
        "description": "Simple Json client plugin for Craft3 CMS. The plugin provides a simple Twig extension which allows you to fetch a Json url and return it to your Twig template.",
        "hasCpSettings": false,
        "hasCpSection": false,
        "documentationUrl": "https://github.com/xuanvan229/craft3-jsonclient/blob/master/README.md",
        "changelogUrl": "https://github.com/xuanvan229/craft3-jsonclient/blob/master/CHANGELOG.md",
        "class": "loca\\jsonclient\\JsonClientPlugin"
    }
}
